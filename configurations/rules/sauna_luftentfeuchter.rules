// Imports
import org.openhab.core.library.types.*
import org.openhab.core.persistence.*
import org.openhab.model.script.actions.*

// Global Variables

// Constants used to generate this rule
// HumidityHigh == 62
// HumidityLow == 57

rule "Sauna Luftentfeuchter"
when
    Item Humidity_UG_Sauna changed
then
  if (((Humidity_UG_Sauna.state >= 62) || (Humidity_UG_Sauna.state >= 57))) {
    postUpdate(UG_Sauna_Luftentfeuchter, ON)
  } else {
    postUpdate(UG_Sauna_Luftentfeuchter, OFF)
  }
end


