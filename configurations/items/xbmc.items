Switch UG_Hannes_XBMC_Screensaver "Raspi Screensaver"   (UG_Hannes)             { xbmc="<[#hannes|Screensaver.State]" }

String EG_Kueche_XBMC_State	"Pixie Status: [%s]" <video>	(EG_Kueche)		{ xbmc="<[#kueche|Player.State]" }
String EG_Kueche_XBMC_Title	"Pixie Title: [%s]"	(EG_Kueche)		{ xbmc="<[#kueche|Player.Title]" }
Dimmer EG_Kueche_XBMC_Volume    "Pixie Volume [%.1f]"   (EG_Kueche)             { xbmc="=[#kueche|Application.Volume]" }
